@page
@model Sync1Demo.Pages.Applicants.ApplicantCreateModel
@{
}


<form method="post">
    <div class="form-group">
        <label asp-for=Applicant.Name>Sender's Name</label>
        <input asp-for=Applicant.Name class="form-control" type="text" placeholder="Name">
    </div>
    <div class="form-group">
        <label asp-for=Applicant.Email>Email Address</label>
        <input asp-for=Applicant.Email class="form-control" type="text" placeholder="Email">
    </div>
    <div class="form-group">
        <label asp-for=Applicant.PhoneNumber>Phone Number</label>
        <input asp-for=Applicant.PhoneNumber class="form-control" type="text" >
    </div>
    <div class="form-group">
        <label asp-for=Applicant.LoanAmount>Loan Amount</label>
        <input asp-for=Applicant.LoanAmount class="form-control" type="text">
    </div>
    <button type="submit" class="btn btn-dark">Submit</button>
</form>